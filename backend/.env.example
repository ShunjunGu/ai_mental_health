# 服务器配置
PORT=57214
NODE_ENV=development

# JWT密钥配置 - 生产环境请使用强随机密钥
# 生成命令: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=ai-mental-health-secret-key-change-in-production
JWT_EXPIRES_IN=24h

# 数据库配置 (可选 - MongoDB)
MONGODB_URI=mongodb://localhost:27017/ai-mental-health

# CORS配置
CORS_ORIGIN=http://localhost:5173

# 日志级别
LOG_LEVEL=info
